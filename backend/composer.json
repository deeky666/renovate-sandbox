        "twig/intl-extra": "3.10.0",
        "twig/twig": "3.14.2",
        "webonyx/graphql-php": "15.12.2"
    },
    "require-dev": {
        "doctrine/coding-standard": "12.0.0",
        "dvdoug/behat-code-coverage": "5.3.4",
        "friends-of-behat/symfony-extension": "2.5.0",
        "goetas/jms-serializer-phpstan-extension": "1.0.1",
        "jiripudil/phpstan-sealed-classes": "1.2.0",
        "phpstan/phpstan": "1.11.11",
        "phpstan/phpstan-deprecation-rules": "1.2.0",
        "phpstan/phpstan-doctrine": "1.5.0",
        "phpstan/phpstan-symfony": "1.4.8",
        "phpunit/phpunit": "10.5.20",
        "symfony/stopwatch": "7.2.4",
        "symfony/web-profiler-bundle": "7.2.4"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "metasyntactical/composer-plugin-license-check": true,
            "symfony/*": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Fixture\\": "fixtures/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feature\\": "behat/",
            "Lint\\": "lint/"
        }
    },
    "replace": {
        "overblog/graphiql-bundle": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php83": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "vendor-fixes": [
            "# Symfony HTTP Client: curl connection timeout fix (https://github.com/symfony/symfony/issues/48513)",
            "sed -i 's/\\\\CURLOPT_CONNECTTIMEOUT,//g' vendor/symfony/http-client/CurlHttpClient.php"
        ],
        "post-install-cmd": [
            "@vendor-fixes",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@vendor-fixes",
            "@auto-scripts"
        ],
        "install-codestandards": [
            "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "runtime": {
            "error_handler": "App\\Infrastructure\\Runtime\\SymfonyErrorHandler"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        },
        "metasyntactical/composer-plugin-license-check": {
            "whitelist": [
                "Apache-2.0",
                "Artistic-1.0",
                "MIT",
                "BSD-3-Clause",
                "BSD-2-Clause",
                "LGPL-3.0",
                "LGPL-2.1",
                "LGPL-3.0-or-later"
            ]
        }
    }
}
