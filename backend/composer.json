        "symfony/security-bundle": "7.2.3",
        "symfony/security-core": "7.2.*",
        "symfony/security-http": "7.2.*",
        "symfony/serializer": "7.2.8",
        "symfony/service-contracts": "3.5.1",
        "symfony/string": "7.2.*",
        "symfony/translation": "7.2.4",
        "symfony/twig-bridge": "7.2.*",
        "symfony/twig-bundle": "7.2.0",
        "symfony/validator": "7.2.4",
        "symfony/var-dumper": "7.2.*",
        "symfony/yaml": "7.2.3",
        "thecodingmachine/graphqlite": "8.0.0",
        "thecodingmachine/graphqlite-bundle": "7.1.0",
        "tilleuls/url-signer-bundle": "2.2.1",
