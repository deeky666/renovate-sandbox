{
    "name": "thevea/app",
    "description": "Abrechnungs- & Praxissoftware - Die digitale und v√∂llig neue Art der Praxisverwaltung",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "8.3.*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "azuyalabs/yasumi": "^2.7",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "dompdf/dompdf": "^2.0",
        "fakerphp/faker": "^1.20",
        "firebase/php-jwt": "^6.4",
        "giggsey/libphonenumber-for-php": "^8.13",
        "gmostafa/php-graphql-client": "^1.13",
        "jms/serializer-bundle": "^5.0",
        "league/flysystem-bundle": "^3.0",
        "league/flysystem-sftp-v3": "^3.6",
        "metasyntactical/composer-plugin-license-check": "^2.1",
        "nadar/quill-delta-parser": "^3.3",
        "picqer/php-barcode-generator": "^2.0",
        "ramsey/uuid-doctrine": "^2.0",
        "sentry/sentry-symfony": "^5.0",
        "symfony/amazon-mailer": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/doctrine-messenger": "7.2.*",
        "symfony/flex": "^2.2",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/messenger": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/serializer": "7.2.*",
        "symfony/translation": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/validator": "7.2.*",
        "symfony/yaml": "7.2.*",
        "thecodingmachine/graphqlite-bundle": "^7.1",
        "tilleuls/url-signer-bundle": "^2.2",
        "twig/extra-bundle": "^3.5",
        "twig/intl-extra": "^3.5",
        "twig/twig": "^3.4"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0",
        "dvdoug/behat-code-coverage": "^5.3",
        "friends-of-behat/symfony-extension": "^2.3",
        "goetas/jms-serializer-phpstan-extension": "^1.0",
        "jiripudil/phpstan-sealed-classes": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^10",
        "symfony/stopwatch": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "metasyntactical/composer-plugin-license-check": true,
            "symfony/*": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Fixture\\": "fixtures/",
            "Feature\\": "behat/"
        }
    },
    "replace": {
        "overblog/graphiql-bundle": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php83": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "vendor-fixes": [
            "# Symfony HTTP Client: curl connection timeout fix (https://github.com/symfony/symfony/issues/48513)",
            "sed -i 's/\\\\CURLOPT_CONNECTTIMEOUT,//g' vendor/symfony/http-client/CurlHttpClient.php"
        ],
        "post-install-cmd": [
            "@vendor-fixes",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@vendor-fixes",
            "@auto-scripts"
        ],
        "install-codestandards": [
            "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "runtime": {
            "error_handler": "App\\Infrastructure\\Runtime\\SymfonyErrorHandler"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        },
        "metasyntactical/composer-plugin-license-check": {
            "whitelist": [
                "Apache-2.0",
                "Artistic-1.0",
                "MIT",
                "BSD-3-Clause",
                "BSD-2-Clause",
                "LGPL-3.0",
                "LGPL-2.1",
                "LGPL-3.0-or-later"
            ]
        }
    }
}
